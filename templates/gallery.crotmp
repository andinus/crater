<:use 'templates/base.crotmp'>
<|page(.title)>
<div class="gallery">
    <@gallery : $i>

    <?{ $i.<type> eq 'img' }>
    <img src="<$i.<src>>">
    </?>

    <?{ $i.<type> eq 'text' }>
